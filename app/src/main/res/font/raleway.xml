<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!-- regular -->
    <font
        android:font="@font/raleway_regular"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/raleway_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- medium -->
    <font
        android:font="@font/raleway_medium"
        android:fontStyle="italic"
        android:fontWeight="400"

        app:font="@font/raleway_medium"
        app:fontStyle="italic"
        app:fontWeight="400" />

    <!-- bold -->
    <font
        android:font="@font/raleway_bold"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/raleway_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <!-- semibold -->
    <font
        android:font="@font/raleway_semibold"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/raleway_semibold"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <!-- light -->
    <font
        android:font="@font/raleway_light"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/raleway_light"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <!-- black -->
    <font
        android:font="@font/raleway_black"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/raleway_black"
        app:fontStyle="normal"
        app:fontWeight="700" />

</font-family>